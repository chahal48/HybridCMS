@model HybridCMSEntities.AssetEntity
@{
    ViewBag.Title = Model.Name ?? "Page Not Found";
    Layout = "~/Views/Shared/_CMSLayout.cshtml";
}
@section styles{
    <style>
        .for-page-header {
            padding-top: 12rem;
            padding-bottom: 6rem;
            background: url(Upload/@Model.ProfilePicture) top right no-repeat !important;
            background-size: cover !important;
        }
    </style>
}

@if (Model.AssetTypeId == HybridCMSEntities.AssetType.Blog)
{
    Html.RenderAction("AssetBlogPartial", "Asset", Model);
}
else if (Model.AssetTypeId == HybridCMSEntities.AssetType.Page)
{
    Html.RenderAction("AssetPagePartial", "Asset", Model);
}