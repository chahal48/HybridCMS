@model HybridCMSEntities.AssetEntity



<!-- Blog Header Start -->
<div class="container-xxl py-6" style="padding-bottom:0 !important;">
    <div class="container">
        <div class="section-header text-center mx-auto wow fadeInUp" data-wow-delay="0.1s" style="max-width: 500px;">
            <div class="row col-lg-12 col-md-12">
                <small class="me-3">
                    <i class="fa fa-user text-primary me-2"></i>@Model.AuthorName &nbsp; <i class="fa fa-calendar text-primary me-2"></i>@Model.CreatedOn.ToString("ddd, dd MMM yyy")
                </small>
            </div>
            <h4 >@Model.AssetName</h4>
        </div>
        <div class="col-lg-12 col-md-12 mb-3 border-top wow fadeInUp" data-wow-delay="0.1s"></div>
    </div>
</div>
<!-- Blog Header End -->
<!-- Blog Start -->
@if (!string.IsNullOrEmpty(Model.Description))
{
    <div class="container-xxl" style="padding-top:0 !important;">
        <div class="container">
            <div class="row g-5 justify-content-center">
                <div class="col-lg-10 col-md-12 wow fadeInUp" data-wow-delay="0.5s">
                    @Html.Raw(@Model.Description)
                </div>
            </div>
        </div>
    </div>

}
<!-- Blog End -->